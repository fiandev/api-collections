{
"buildCommand":"pip install -r requirements.txt && python app.py",
  "builds": [
    {
      "src": "./app.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
